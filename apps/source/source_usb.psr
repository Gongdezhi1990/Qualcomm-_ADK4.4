&04f8 = 1004
//  BOOTMODE_KEY_TABLE_1+1: ONCHIP_HCI_CLIENT = 1
&04f9 = 0001
//  BOOTMODE_KEY_TABLE_1+2: USB_VM_CONTROL = 1
&04fa = 0001
//  BOOTMODE_KEY_TABLE_1+3: VM_DISABLE = 0
&04fb = 0000
